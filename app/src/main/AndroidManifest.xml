<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bz0209.happyidiom">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar">
        <uses-library android:name="android.test.runner" />

        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.StudyActivity" />
        <activity android:name=".activity.StudyAnimalActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.CollectActivity" />
        <activity android:name=".activity.GameActivity" />
        <activity android:name=".activity.GameGuessActivity" />
        <activity android:name=".activity.PlayGuessActivity" />
        <activity android:name=".activity.SearchActivity"></activity>
    </application>

</manifest>